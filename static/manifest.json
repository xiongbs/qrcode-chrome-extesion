{
    "name": "Qrcode",
    "description": "A simple qrcode extension powered by svelte",
    "version": "1.0",
    "manifest_version": 3,

    "permissions": ["tabs", "downloads"],
    "action": {
        "default_popup": "index.html"
    },

    "icons": {
        "16": "/image/qrcode_16.png",
        "32": "/image/qrcode_32.png",
        "48": "/image/qrcode_48.png",
        "128": "/image/qrcode_128.png"
    }

}